{
   "AWSTemplateFormatVersion":"2010-09-09",
   "Description": "",
   "Parameters":{
    "parameter1": {
      "Type": "String",
      "Default": "string"
      }
   },
   "Resources":{
     "DeleteMeLambda": {
       "Type" : "AWS::Lambda::Function",
       "Properties" : {
           "Code" : {
             "S3Bucket": "delete-me-please-1234"
           },
           "FunctionName" : "delete_me_lambda",
           "Handler" : "hello.handler",
           "Role" : { "Fn::GetAtt": [ "DeleteMeLambdaRole", "Arn" ] },
           "Runtime" : "python3.7"
         }
     },
    "DeleteMeLambdaRole":{
      "Type" : "AWS::IAM::Role",
      "Properties" : {
          "AssumeRolePolicyDocument" : {
              "Effect": "Allow",
              "Principal": {
                "Service": [ "lambda.amazonaws.com" ]
              }
            },
          "Policies" : [
            {
            "PolicyName": "deleteMe",
            "PolicyDocument": {
              "Version": "2012-10-17",
            {
              "Effect": "Allow",
              "Action": "s3:*",
              "Resource": "arn:aws:s3:::delete-me-please-1234*"
            }
          }
        ]
      }
    }
  }
}
